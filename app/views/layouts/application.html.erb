<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= content_for?(:title) ? yield(:title) : "Curriculo" %></title>
    <meta name="description" content="">
    <meta name="author" content="">
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" %>
    <%= csrf_meta_tags %>
    
  </head>
  <body>
   <header role="banner" class="navbar navbar-fixed-top navbar-inverse">
      <div class="container">
        <div class="navbar-header">
          <button data-toggle="collapse-side" data-target=".side-collapse" data-target-2=".side-collapse-container" type="button" class="navbar-toggle pull-left"><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></button>
        </div>
        <div class="navbar-inverse side-collapse in">
          <nav role="navigation" class="navbar-collapse">
            <%= render "layouts/navigation" %>
          </nav>
        </div>
      </div>
    </header>
    <div class="conteudo container side-collapse-container">
      <div class="row">
        <%= render "layouts/messages" %>
        <%= yield %>
      </div>
    </div>
    
    <footer class="w3-container w3-black w3-margin-top">
      <h5>Curriculo</h5>
      <p>Contato:</p>
    </footer>

    
    
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true  %>

  </body>
    

</html>

